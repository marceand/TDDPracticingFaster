<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.marceme.tddfasterpracting"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".imageui.ImageActivity">
        </activity>
        <activity android:name=".nutritionixui.NutritionixActivity">
        </activity>
        <activity android:name=".feedbackui.FeedbackActivity">
        </activity>
        <activity android:name=".ExampleActivity">
        </activity>
    </application>

</manifest>